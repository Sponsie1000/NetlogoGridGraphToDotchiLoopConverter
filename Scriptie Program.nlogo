to Place-Done

  tick

  ask patches [if pcolor = blue [set pcolor green]]

  createNodes

  let rules "error" let nodeSize "error"



  ifelse Ruleset = "Basic Rules" [set rules 0 set nodeSize 5 ]
  [ ifelse Ruleset = "No Single White Cells" [set rules 1 set nodeSize 7]
    [ ifelse Ruleset = "No 4-Adjacent Black Cells" [set rules 2 set nodeSize 10]
      []
    ]]
  if rules = "error" [set message "Hier is iets mis gegaan met rule-set selecteren!"]



  calcNewWorld nodeSize
  ;; this includes the actual action of resizing the world
  ;; and it includes storeNodes

  restoreNodes

  plaatsRooms(rules)

  maakZwart

end

to maakZwart

  ask patches with [pxcor = 0 or pxcor = max-pxcor or pycor = 0 or pycor = max-pycor] [set pcolor black]
  let change? true

  while [change?]
  [
    let change false
    ask patches with [pcolor = 9]
    [
      ;;if [pcolor] of one-of neighbors4 = black [set pcolor black set change true]
      if any? neighbors4 with [pcolor = black] [set pcolor black set change true]
    ]
    if change = false [set change? false]

  ]

  ask patches with [pcolor = 9] [set pcolor 6]


end

to storeNodes [NminX NminY nodeSize]

  set nodebase []

  ask nodes
  [
    let x floor ((pxcor - NminX) * nodeSize + round (nodeSize / 2))
    let y floor ((pycor - NminY) * nodeSize + round (nodeSize / 2))

    set nodebase lput (list x y NZWO connections category) nodebase

    ; set NZWO lput "N" NZWO
  ]

end

to restoreNodes

  create-nodes (length nodebase) [ set NZWO [] set placed? false ]

  foreach nodebase
  [ info ->

    ask one-of nodes with [shape != "circle"]
    [
      move-to patch item 0 info item 1 info
      set area 0

      set NZWO item 2 info
      set connections item 3 info
      set category item 4 info

      set shape "circle" set size 1.5 set color red set hidden? true
    ]
  ]

end

to calcNewWorld [nodeSize]

  let NminX 0 let NmaxX 0
  let NminY 0 let NmaxY 0

  ask one-of nodes with-min [pxcor] [set NminX pxcor]
  ask one-of nodes with-max [pxcor] [set NmaxX pxcor]

  ask one-of nodes with-min [pycor] [set NminY pycor]
  ask one-of nodes with-max [pycor] [set NmaxY pycor]

  let maxX (NmaxX * nodeSize - NminX * nodeSize + nodeSize + 1)
  let maxY (NmaxY * nodeSize - NminY * nodeSize + nodeSize + 1)


  storeNodes NminX NminY nodeSize

  if Rescale?
  [
    let patchTotalX (NmaxX - NminX + 1) * nodeSize + 2
    let patchTotalY (NmaxY - NminY + 1) * nodeSize + 2
    let patchSize "blap"
    ifelse patchTotalX >= patchTotalY
    [set patchSize (429 / patchTotalX)]
    [set patchSize (429 / patchTotalY)]
    set-patch-size patchSize
  ]

  resize-world 0 maxX 0 maxY

  drawGrid


end

to createNodes

  let aantal count patches with [pcolor = green]

  create-nodes aantal [ set category "node" set NZWO [] set placed? false ]

  while [count patches with [pcolor = green] != 0]
  [
    ask one-of nodes with [connections = 0]
    [
      move-to one-of patches with [pcolor = green]
      set pcolor 9 set connections connected?
      ifelse connections = 4 [set NZWO ["N""Z""W""O"]] [checkSurrounding]
      set shape "circle" set size 1.5 set color red ;set hidden? true
    ]
  ]

  set aantal count patches with [pcolor = brown]
  create-nodes aantal [ set category "hall" set NZWO [] set placed? false ]

  while [count nodes with [shape != "circle"] != 0]
  [
    ask one-of nodes with [shape != "circle"]
    [
      move-to one-of patches with [pcolor = brown and bezet != 1]
      ;ifelse connections = 4 [set NZWO ["N""Z""W""O"]] [checkSurrounding]
      ifelse area = 2 [set NZWO ["N""Z"] ]  [set NZWO ["W""O"] ]

      set shape "circle" set size 1.5 set color red set bezet 1 ;set hidden? true
    ]
  ]

end

to plaatsRooms[rules]

  while [any? nodes with [placed? = false] ]
  [
    let aantal (((item 0 item 0 item 0 item 0 item rules database) * -2) + 1)
    if (item 0 item 0 item 0 item 0 item rules database) != (item 0 item aantal item 0 item 0 item rules database) [set aantal (aantal + 1)]

    maakTurtles aantal
    ask one-of nodes with [placed? = false]
    [
      set pcolor 9
      let roomrot calcOrient
      let room item 0 roomrot let rot item 1 roomrot

      let i 0
      while [any? turtles with [shape != "circle"] ]
      [
        ask one-of turtles with [shape != "circle"]
        [
          move-to patch
          ([pxcor] of myself + (item 0 item i item rot item room item rules database))
          ([pycor] of myself + (item 1 item i item rot item room item rules database))
          set pcolor (item 3 item i item rot item room item rules database)

          ifelse item 2 item i item rot item room item rules database = " "  [die]
          [
            set shape "circle"
            ifelse item 2 item i item rot item room item rules database = "X" [set color black] [set color white]
          ]
        ]
        set i (i + 1)
      ]
      set placed? true
    ]
  ]

end

to checkSurrounding

  if [pcolor] of (patch (xcor    ) (ycor + 1)) = brown [set NZWO lput "N" NZWO]
  if [pcolor] of (patch (xcor    ) (ycor - 1)) = brown [set NZWO lput "Z" NZWO]
  if [pcolor] of (patch (xcor - 1) (ycor    )) = brown [set NZWO lput "W" NZWO]
  if [pcolor] of (patch (xcor + 1) (ycor    )) = brown [set NZWO lput "O" NZWO]

end

to-report calcOrient  ; bepaalt voor iedere node hoe de rotaties dienen te zijn

  let room "ERROR: Ging iets mis bij room bepalen!"
  let rot  "ERROR: Ging iets mis bij rotatie bepalen!"

  if connections = 0
  [
    set room 4
    if NZWO = ["N""Z"] [set rot 1]
    if NZWO = ["W""O"] [set rot 0]
  ]

  if connections = 1
  [
    set room 5
    if NZWO = ["Z"] [set rot 0]
    if NZWO = ["O"] [set rot 1]
    if NZWO = ["N"] [set rot 2]
    if NZWO = ["W"] [set rot 3]
  ]

  if connections = 2
  [
    ifelse NZWO = ["N""Z"] or NZWO = ["W""O"]
    [set room 0]
    [set room 1]
  ]

  if room = 0
  [
    if NZWO = ["N""Z"] [set rot 1]
    if NZWO = ["W""O"] [set rot 0]
  ]

  if room = 1
  [
    if NZWO = ["N""W"] [set rot 2]
    if NZWO = ["Z""W"] [set rot 3]
    if NZWO = ["N""O"] [set rot 1]
    if NZWO = ["Z""O"] [set rot 0]
  ]

  if connections = 3
  [
    if NZWO = ["N""Z""W"] [set room 2 set rot 3]
    if NZWO = ["Z""W""O"] [set room 2 set rot 0]
    if NZWO = ["N""W""O"] [set room 2 set rot 2]
    if NZWO = ["N""Z""O"] [set room 2 set rot 1]
  ]

  if connections = 4 [set room 3 set rot 0]

  report (list room rot)

end

to maakTurtles [aantal]

  let i 0
  while [i < aantal * aantal]
  [
    create-turtles 1 [set size 0.7]
    set i (i + 1)
  ]

 ;set message count turtles

end
