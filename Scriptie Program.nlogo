to drawGrid

  no-display

  ask patches [set pcolor 9 set area -1 set connected? 0]

  let HorizontalLines max-pxcor
  let VerticalLines max-pycor

  create-turtles HorizontalLines
  [set xcor 0.5 set ycor 0.5 set color black set heading 90 set size 2 ]

  let id [who] of one-of turtles with-min [who]
  let blep min-pxcor

  while [blep != max-pxcor]
  [
    ask turtle id
    [move-to patch blep 0]
    set id (id + 1)
    set blep (blep + 1)
  ]

  ask turtles
  [
    forward 0.5
    set heading 0
    forward 0.5
    pen-down
    forward 1

    while [ycor != 0.5] [forward 1]

    pen-up
    die
  ]


  create-turtles VerticalLines
  [set xcor 0.5 set ycor 0.5 set color black set heading 0 set size 2 ]

  set id [who] of one-of turtles with-min [who]

  set blep min-pycor
  while [blep != max-pycor]
  [
    ask turtle id
    [move-to patch 0 blep]
    set id (id + 1)
    set blep (blep + 1)

  ]

  ask turtles
  [
    forward 0.5
    set heading 90
    forward 0.5
    pen-down
    forward 1

    while [xcor != 0.5] [forward 1]

    pen-up
    die
  ]

  display

end

to place-Activate

  if Object = "Vertex"
  [
    if eerste != 0 [ask eerste [set pcolor green] set eerste 0 set corridor-state 0]
    let once true
    while [mouse-down?]
    [
      while [once]
      [
        set message ""
        ask patch mouse-xcor mouse-ycor
        [
          let check true
          ask neighbors [if pcolor = green or pcolor = brown [set check false]]
          ifelse check
          [set pcolor green set area 0]
          [set message "Placing a vertex here would be too close to other object(s)!"]
          set once false
        ]
      ]
    ]
  ]

  if Object = "Edge"
  [
    let once true
    while [mouse-down?]
    [
      while [once]
      [
        set message ""
        ask patch mouse-xcor mouse-ycor
        [
          ifelse pcolor = green and corridor-state = 0
          [
            set pcolor blue
            set corridor-state 1
            set eerste self
          ]
          [
            ifelse pcolor = green and corridor-state = 1
            [
              set tweede self

              connect

              ask eerste [set pcolor green]
              set eerste 0 set tweede 0
              set corridor-state 0
            ]
            [ if self = eerste [ set pcolor green set eerste 0 set corridor-state 0 ] ]
          ]

          set once false
        ]
      ]
    ]
  ]

  if Object = "Erase"
  [
    if eerste != 0 [ask eerste [set pcolor green] set eerste 0 set corridor-state 0]
    let once true
    while [mouse-down?]
    [
      while [once]
      [
        set message ""
        ask patch mouse-xcor mouse-ycor
        [
          set eerste self

          if area = 0
          [
            ifelse connected? > 0
            [set message "Please erase all connected edges of this vertex first!"]
            [
              set pcolor 9
              set area -1
            ]
          ]
          if area = 1   ;; horizontaal connected
          [
            let patchcurr patch ([pxcor] of eerste - 1) ([pycor] of eerste)
            while [ [area] of patchcurr > 0 ] ;; zoek linker-verbinding
            [
              ask patchcurr [set pcolor 9 set area -1]
              set patchcurr patch ([pxcor] of patchcurr - 1) ([pycor] of eerste)
            ]
            ask patchcurr [set connected? (connected? - 1)]
            set patchcurr patch ([pxcor] of eerste + 1) ([pycor] of eerste)

            while [ [area] of patchcurr > 0 ] ;; zoek rechter-verbinding
            [
              ask patchcurr [set pcolor 9 set area -1]
              set patchcurr patch ([pxcor] of patchcurr + 1) ([pycor] of eerste)
            ]
            ask patchcurr [set connected? (connected? - 1)]
            ask eerste [set pcolor 9 set area -1]
          ]
          if area = 2   ;; verticaal connected
          [
            let patchcurr patch ([pxcor] of eerste) ([pycor] of eerste - 1)
            while [ [area] of patchcurr > 0 ] ;; zoek onder-verbinding
            [
              ask patchcurr [set pcolor 9 set area -1]
              set patchcurr patch ([pxcor] of eerste) ([pycor] of patchcurr - 1)
            ]
            ask patchcurr [set connected? (connected? - 1)]
            set patchcurr patch ([pxcor] of eerste) ([pycor] of eerste + 1)

            while [ [area] of patchcurr > 0 ] ;; zoek boven-verbinding
            [
              ask patchcurr [set pcolor 9 set area -1]
              set patchcurr patch ([pxcor] of eerste) ([pycor] of patchcurr + 1)
            ]
            ask patchcurr [set connected? (connected? - 1)]
            ask eerste [set pcolor 9 set area -1]
          ]

          set eerste 0
          set once false
        ]
      ]
    ]
  ]

end

to connect

  let check true
  ifelse ([pxcor] of eerste) = ([pxcor] of tweede)
  [
    ifelse [pycor] of eerste > [pycor] of tweede
    [
      ask patches with [pycor > [pycor] of tweede and pycor < [pycor] of eerste and pxcor = [pxcor] of eerste ]
      [
        if pcolor = green or pcolor = brown [ set check false]
        if [area] of patch (pxcor + 1) pycor > -1 or [area] of patch (pxcor - 1) pycor > -1 [set check false]
      ]
      ifelse check
      [
        ask patches with [pycor > [pycor] of tweede and pycor < [pycor] of eerste and pxcor = [pxcor] of eerste ] [set pcolor brown set area 2]
        ask eerste [set connected? (connected? + 1)] ask tweede [set connected? (connected? + 1)]
      ]
      [ set message "This edge would be too close to other objects!"]
    ]
    [
      ask patches with [pycor < [pycor] of tweede and pycor > [pycor] of eerste and pxcor = [pxcor] of eerste ]
      [
        if pcolor = green or pcolor = brown [set check false]
        if [area] of patch (pxcor + 1) pycor > -1 or [area] of patch (pxcor - 1) pycor > -1 [set check false]
      ]
      ifelse check
      [
        ask patches with [pycor < [pycor] of tweede and pycor > [pycor] of eerste and pxcor = [pxcor] of eerste ] [set pcolor brown set area 2]
        ask eerste [set connected? (connected? + 1)] ask tweede [set connected? (connected? + 1)]
      ]
      [ set message "This edge would be too close to other objects!!"]
    ]
  ]
  [
    ifelse ([pycor] of eerste) = ([pycor] of tweede)
    [
      ifelse ([pxcor] of eerste) > ([pxcor] of tweede)
      [
        ask patches with [(pxcor > [pxcor] of tweede) and (pxcor < [pxcor] of eerste) and (pycor = [pycor] of eerste) ]
        [
          if pcolor = green or pcolor = brown [set check false]
          if [area] of patch pxcor (pycor + 1) > -1 or [area] of patch pxcor (pycor - 1) > -1 [set check false]
        ]
        ifelse check
        [
          ask patches with [pxcor > [pxcor] of tweede and pxcor < [pxcor] of eerste and pycor = [pycor] of eerste ] [set pcolor brown set area 1]
          ask eerste [set connected? (connected? + 1)] ask tweede [set connected? (connected? + 1)]
        ]
        [ set message "This edge would be too close to other objects!"]
      ]
      [
        ask patches with [pxcor < [pxcor] of tweede and pxcor > [pxcor] of eerste and pycor = [pycor] of eerste ]
        [
          if pcolor = green or pcolor = brown [set check false]
          if [area] of patch pxcor (pycor + 1) > -1 or [area] of patch pxcor (pycor - 1) > -1 [set check false]
        ]
        ifelse check
        [
          ask patches with [pxcor < [pxcor] of tweede and pxcor > [pxcor] of eerste and pycor = [pycor] of eerste ] [set pcolor brown set area 1]
          ask eerste [set connected? (connected? + 1)] ask tweede [set connected? (connected? + 1)]
        ]
        [ set message "This edge would be too close to other objects!"]
      ]
    ]
    [ set message "These nodes do not share any axis!" ]
  ]

end


